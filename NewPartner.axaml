<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="800" Height="450"
        x:Class="demoDrozdov.NewPartner"
		x:CompileBindings="False"
        Title="NewPartner">
<Grid Name="partnerModel">
	<Button HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,170,30"
			Content="Создать партнера" Background="#67BA80" Click="createPartner">
		
	</Button>
	<Button HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,30,20,0"
			Content="Список партнеров" Background="#67BA80" Click="goToPartners">

	</Button>
	<StackPanel Width="400" Height="400" Background="#F4E8D3" HorizontalAlignment="Left" Margin="50">
		<TextBox Watermark="Тип партнера" Name="partnerType" Text="{Binding Type}"></TextBox>
		<TextBox Watermark="Название" Margin="0,20,0,0" Name="partnerName" Text="{Binding Partnername}"></TextBox>
		<TextBox Watermark="ФИО директора" Margin="0,20,0,0" Name="directorName" Text="{Binding Director}"></TextBox>
		<TextBox Watermark="Номер телефона" Margin="0,20,0,0" Name="partnerPhone" Text="{Binding Partnerphone}"></TextBox>
		<TextBox Watermark="Рейтинг" Margin="0,20,0,0" Name="partnerRating" Text="{Binding Rating}"></TextBox>
		<TextBox Watermark="Скидка партнера" Margin="0,20,0,0" Name="partnerDiscount" Text="{Binding Discount}"></TextBox>
		<TextBox Watermark="Почта" Margin="0,20,0,0" Name="partnerEmail" Text="{Binding Partneremail}"></TextBox>
		<TextBox Watermark="Адрес" Margin="0,20,0,0" Name="partnerAdress" Text="{Binding Partneradress}"></TextBox>
	</StackPanel>		
</Grid>
</Window>
